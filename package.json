{
  "name": "@uwu-codes/core",
  "version": "1.1.13",
  "description": "A core library for my bots.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf build && npm run lint && tsc && npm run copy && echo Build Finished",
    "lint": "eslint src --ext .ts --fix",
    "circ": "dpdm --tree=false --circular=true --warning=false -T .",
    "copy": "cp -r src/@types build && cp src/general/EvalUtil.d.ts build/general",
    "prepack": "check-for-leaks",
    "prepush": "check-for-leaks"
  },
  "author": "Donovan_DMC",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UWUCode/Core.git"
  },
  "bugs": {
    "url": "https://github.com/UWUCode/Core/issues"
  },
  "homepage": "https://github.com/UWUCode/Core#readme",
  "dependencies": {
    "@types/mongodb": "^3.6.16",
    "deasync": "^0.1.21",
    "dot-object": "^2.1.4",
    "eris": "github:abalabahaha/eris#dev",
    "fs-extra": "^10.0.0",
    "ioredis": "^4.27.3",
    "json5": "^2.2.0",
    "language": "npm:@uwu-codes/language@^1.0.4",
    "logger": "npm:@uwu-codes/logger@^1.0.22",
    "mongodb": "^3.6.8",
    "source-map-support": "^0.5.19",
    "utilities": "npm:@uwu-codes/utils@^1.0.37"
  },
  "devDependencies": {
    "@types/deasync": "^0.1.1",
    "@types/dot-object": "^2.1.2",
    "@types/fs-extra": "^9.0.11",
    "@types/json5": "^0.0.30",
    "@types/node": "^15.6.0",
    "@uwu-codes/eslint-config": "^1.0.17",
    "@uwu-codes/tsconfig": "^1.0.1",
    "check-for-leaks": "^1.2.1",
    "eslint": "^7.27.0",
    "husky": "^6.0.0",
    "typescript": "^4.3.1-rc"
  },
  "optionalDependencies": {
    "clustering": "npm:@uwu-codes/clustering@^1.0.10"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.x",
    "npm": ">=7.x"
  },
  "os": [
    "linux"
  ],
  "engineStrict": true
}
